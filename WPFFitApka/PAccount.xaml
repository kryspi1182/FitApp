        <Grid Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>
            <StackPanel  Grid.Row="0"  VerticalAlignment="Top" HorizontalAlignment="Right" Orientation="Horizontal" Margin="10,25,10,0" >
                <iconPacks:PackIconMaterial Kind="Logout" 
                                        Foreground="#FF217380"
                                        HorizontalAlignment="Right" VerticalAlignment="Center"  Margin="0,0,10,0" Width="16"
                                        />
                <ToggleButton Click="Logout_Button" Content="Wyloguj" FontSize="20" Height="40" >
                    <ToggleButton.Style>
                        <Style TargetType="{x:Type ToggleButton}">
                            <Setter Property="Foreground" Value="#FF217380"/>
                            <Setter Property="FontFamily" Value="Lato"/>
                            <Setter Property="FontSize" Value="20"/>
                            <Setter Property="FontWeight" Value="Light"/>
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="BorderThickness" Value="0"/>

                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="{x:Type ToggleButton}">
                                        <Border Background="{TemplateBinding Background}" 
                            CornerRadius="20"
                            >
                                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>

                                        </Border>
                                    </ControlTemplate>

                                </Setter.Value>

                            </Setter>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="White" />
                                    <Setter Property="Opacity" Value="0.4"/>
                                    <Setter Property="Foreground" Value="Red"/>

                                </Trigger>
                                <Trigger Property="IsChecked" Value="True">
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </ToggleButton.Style>
                </ToggleButton>

            </StackPanel>
            <Grid Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"></ColumnDefinition>
                    <ColumnDefinition Width="auto"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"></RowDefinition>
                    <RowDefinition Height="*"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0" Grid.Column="0" HorizontalAlignment="Center" Margin="40, 10">
                <Rectangle Grid.Row="0" Grid.Column="0" RadiusX="10" RadiusY="10" Fill="white"   Width="200" Margin="50, 0, 0, 0"/>
                <StackPanel  Grid.Row="0" Grid.Column="0" Orientation="Vertical"  VerticalAlignment="Center" HorizontalAlignment="Center">
                    <StackPanel Orientation="Vertical">
                        <Label HorizontalAlignment="Center" Padding="0,10" Margin="50,0,0,0" FontSize="15">Zapotrzebowanie</Label>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                            <TextBox Foreground="#FFFF9700"  
                                 FontSize="20" FontWeight="Bold" 
                                 Text="" Name="userKcal" 
                                 Background="Transparent"
                                 TextAlignment="Right"
                                 Padding="10"
                                 Margin="5"
                                 VerticalAlignment="Center"
                                 />
                            <Label VerticalAlignment="Center" Content="Kcal" FontSize="20" FontWeight="Bold"/>
                        </StackPanel>

                    </StackPanel>
                </StackPanel>
                </Grid>
                <Grid Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" Margin="40,10">
                    <Rectangle Grid.Row="1" Grid.Column="3" RadiusX="10" RadiusY="10" Fill="white" Width="200"  />
                    <StackPanel  Grid.Row="1" Grid.Column="3" Orientation="Vertical">
                        <StackPanel Orientation="Vertical">
                            <Label HorizontalAlignment="Center" Padding="0,10"  FontSize="15">BMI</Label>
                            <TextBox Foreground="#FFFF9700"  
                                 FontSize="20" FontWeight="Bold" 
                                 Text="" Name="userBMI"
                                 Background="Transparent"
                                 TextAlignment="Center"
                                 VerticalAlignment="Center"
                                 Padding="10"
                                 Margin="5"
                             />
                        </StackPanel>
                    </StackPanel>
                </Grid>
                <Grid Grid.Row="1" Grid.Column="0" Margin="10" HorizontalAlignment="Center" VerticalAlignment="Center">
                    <Rectangle  RadiusX="10" RadiusY="10" Fill="white" Margin="50, 0, 0, 0" Width="205" Height="200"/>
                    <Grid Margin="50, 0, 0, 0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto"></RowDefinition>
                            <RowDefinition Height="auto"></RowDefinition>
                            <RowDefinition Height="auto"></RowDefinition>
                            <RowDefinition Height="auto"></RowDefinition>
                            <RowDefinition Height="auto"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label VerticalAlignment="Center"  FontWeight="Bold" FontSize="15" Padding="20, 10" Grid.Row="0" Grid.Column="0">Nazwa</Label>
                        <TextBox Grid.Row="0" Grid.Column="1" FontSize="15" Foreground="#FF217380" Text="" Name="userName" Margin="15, 10" Height="20"></TextBox>
                        <Label VerticalAlignment="Center" Grid.Row="1" Grid.Column="0"  FontWeight="Bold" FontSize="15" Padding="20, 10, 0, 10">Płeć</Label>
                        <TextBox FontSize="15" Grid.Row="1" Grid.Column="1" Foreground="#FF217380" Text="" Name="userSex" Margin="15, 10"></TextBox>
                        <Label VerticalAlignment="Center" Grid.Row="2" Grid.Column="0"  FontWeight="Bold" FontSize="15" Padding="20, 10">Wiek</Label>
                        <TextBox FontSize="15"  Grid.Row="2" Grid.Column="1" Foreground="#FF217380"  Text="" Name="userAge" Margin="15, 10"></TextBox>
                        <Label VerticalAlignment="Center"  Grid.Row="3" Grid.Column="0" FontWeight="Bold" FontSize="15" Padding="20, 10">Waga</Label>
                        <TextBox FontSize="15" Grid.Row="3" Grid.Column="1" Foreground="#FF217380"  Text="" Name="userWeight" Margin="15, 10"></TextBox>
                        <Label VerticalAlignment="Center"  Grid.Row="4" Grid.Column="0" FontWeight="Bold" FontSize="15" Padding="20, 10">Wzrost</Label>
                        <TextBox FontSize="15" Grid.Row="4" Grid.Column="1" Foreground="#FF217380"  Text="" Name="userHeight" Margin="15, 10"></TextBox>
                    </Grid>
                 

                </Grid>
                <Grid Grid.Row="1" Grid.Column="1"  HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10"> 
                    <Rectangle  RadiusX="10" RadiusY="10" Fill="white" Margin="10,0,0,0" Width="240" Height="200"/>
                    <Grid Margin="15, 0, 0, 0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto"></ColumnDefinition>
                            <ColumnDefinition Width="auto"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto"></RowDefinition>
                            <RowDefinition Height="auto"></RowDefinition>
                            <RowDefinition Height="auto"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label   FontWeight="Bold" FontSize="15" Grid.Column="0" Grid.Row="0" Padding="20,10,10, 0" Margin="0,10,0, 0">Tłuszcze</Label>
                        <TextBox FontSize="15" Foreground="#FF217380"  Grid.Row="0" Grid.Column="1" Text="" Padding="20,10,10, 0" Margin="0,10,0, 0"  Name="userFat" ></TextBox>
                        <Label   FontWeight="Bold" FontSize="15" Grid.Column="0" Grid.Row="1" Padding="20,10,10, 0" Margin="0, 10, 0, 0">Węglowodany</Label>
                        <TextBox FontSize="15" Foreground="#FF217380"  Grid.Row="1" Grid.Column="1" Text="" Padding="20,10,10, 0" Margin="0,10,0, 0"  Name="userCarbohydrate"></TextBox>
                        <Label  Grid.Row="2" Grid.Column="0" FontWeight="Bold" FontSize="15" Padding="20,10,10, 0" Margin="0,10,0, 0" >Proteiny</Label>
                        <TextBox FontSize="15" Foreground="#FF217380"  Grid.Row="2" Grid.Column="1" Text="" Padding="20,10,10, 0" Margin="0,10,0, 0"  Name="userProtein"></TextBox>
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
